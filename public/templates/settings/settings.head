<div ng-cloak ng-controller="settingsController as ctrl">
    <div ng-if="ctrl.load" class="md-padding">
        <react-component name="EuiProgress" props="{size: 'xs', color: 'primary'}" />
    </div>

    <div ng-if="!ctrl.load && ctrl.settingsTabsProps && !ctrl.apiIsDown && ctrl.apiTableProps.apiEntries.length">
        <react-component name="WzSettingsSideMenu" props="ctrl.settingsTabsProps" />        
    </div>